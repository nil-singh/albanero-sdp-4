#include<bits/stdc++.h>
using namespace std;
int findWays(int n){
	if(n==0 || n==1){
           return 1;
       }
       if(n==2)
       	return 2;
       int dp[n+1];
       dp[0]=0;
       dp[1]=1;
       dp[2]=2;
       for (int i = 3; i <= n; i++){
       	dp[i] = dp[i - 1] + dp[i - 2];
       }
       return dp[n];
}
int main(){
	int n;
	cin>>n;
	int ans = findWays(n);
	cout << ans << endl;
	return 0;
}
