#include <bits/stdc++.h>
#include<string>
using namespace std;

int main(){
    string s1;
    string s2;
    cout<<"Enter the first integer "<<endl;
    cin>>s1;
    cout<<"Enter the second integer of same length "<<endl;
    cin>>s2;
    int diff=0;
    for(int i=0; i<s1.length();i++){
        diff=diff+abs(int(s1[i])-int(s2[i]));
    }

    cout<<"The digit distance between two numbers is "<<diff<<endl;

    return 0;

}
